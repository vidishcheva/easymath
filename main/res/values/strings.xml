    <string name="trig_formulas">
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020<b>Trigonometric functions</b>  \n\n
        \u0020\u0020\u0020\u0020\u0020sin<sup>2</sup><i>x</i> + cos<sup>2</sup><i>x</i> = 1 \n
        \u0020\u0020\u0020\u0020\u0020tg<i>x</i> = sin<i>x</i>/cos<i>x</i> \n
        \u0020\u0020\u0020\u0020\u0020ctg<i>x</i> = cos<i>x</i>/sin<i>x</i> \n
        \u0020\u0020\u0020\u0020\u0020tg<i>x</i>*ctg<i>x</i> = 1 \n
        \u0020\u0020\u0020\u0020\u0020tg<sup>2</sup><i>x</i> + 1 = 1/cos<sup>2</sup><i>x</i> \n
        \u0020\u0020\u0020\u0020\u0020ctg<sup>2</sup><i>x</i> + 1 =  1/sin<sup>2</sup><i>x</i> \n
        \u0020\u0020\u0020\u0020\u0020___________________________________ \n
        \u0020\u0020\u0020\u0020\u0020sin(α + β) = sinα cosβ + cosα sinβ \n
        \u0020\u0020\u0020\u0020\u0020cos(α + β) = cosα cosβ - sinα sinβ \n
        \u0020\u0020\u0020\u0020\u0020tg(α + β) = (tgα + tgβ) / (1 - tgα tgβ) \n
        \u0020\u0020\u0020\u0020\u0020ctg(α + β) = (ctgα ctgβ - 1) / (ctgα + ctgβ) \n
        \u0020\u0020\u0020\u0020\u0020sin(α - β) = sinα cosβ - cosα sinβ \n
        \u0020\u0020\u0020\u0020\u0020cos(α - β) = cosα cosβ + sinα sinβ \n
        \u0020\u0020\u0020\u0020\u0020tg(α - β) = (tgα - tgβ) / (1 + tgα tgβ) \n
        \u0020\u0020\u0020\u0020\u0020ctg(α - β) = (ctgα ctgβ + 1) / (ctgα - ctgβ) \n
        \u0020\u0020\u0020\u0020\u0020___________________________________ \n
        \u0020\u0020\u0020\u0020\u0020sinα + sinβ = 2sin((α + β)/2) * cos((α - β)/2) \n
        \u0020\u0020\u0020\u0020\u0020cosα + cosβ = 2cos((α + β)/2) * cos((α - β)/2) \n
        \u0020\u0020\u0020\u0020\u0020(sinα + cosα)<sup>2</sup> = 1 + sin2α \n
        \u0020\u0020\u0020\u0020\u0020tgα + tgβ = sin(α + β) / cosα cosβ \n
        \u0020\u0020\u0020\u0020\u0020ctgα + ctgβ = sin(α + β) / sinα sinβ \n
        \u0020\u0020\u0020\u0020\u0020___________________________________ \n
        \u0020\u0020\u0020\u0020\u0020sinα - sinβ = 2sin((α - β)/2) * cos((α + β)/2) \n
        \u0020\u0020\u0020\u0020\u0020cosα - cosβ = -2sin((α + β)/2) * sin((α - β)/2) \n
        \u0020\u0020\u0020\u0020\u0020(sinα - cosα)<sup>2</sup> = 1 - sin2α   \n
        \u0020\u0020\u0020\u0020\u0020tgα - tgβ = sin(α - β) / cosα cosβ \n
        \u0020\u0020\u0020\u0020\u0020ctgα - ctgβ = –sin(α - β) / sinα sinβ \n
        \u0020\u0020\u0020\u0020\u0020___________________________________ \n
        \u0020\u0020\u0020\u0020\u0020sinα * sinβ = (cos(α - β) - cos(α + β)) / 2  \n
        \u0020\u0020\u0020\u0020\u0020sinα * cosβ = (sin(α - β) + sin(α + β)) / 2  \n
        \u0020\u0020\u0020\u0020\u0020cosα * cosβ = (cos(α - β) + cos(α + β)) / 2  \n
        \u0020\u0020\u0020\u0020\u0020tgα * tgβ = (cos(α - β) - cos(α + β)) / (cos(α - β) + cos(α + β)) = \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 = (tgα + tgβ) / (ctgα + ctgβ) \n
        \u0020\u0020\u0020\u0020\u0020ctgα * ctgβ = (cos(α - β) + cos(α + β)) / (cos(α - β) - cos(α + β)) = \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 = (ctgα + ctgβ) / (tgα + tgβ)  \n
        \u0020\u0020\u0020\u0020\u0020tgα * ctgβ = (sin(α - β) + sin(α + β)) / (sin(α + β) - sin(α - β)) \n
        \u0020\u0020\u0020\u0020\u0020___________________________________ \n
        \u0020\u0020\u0020\u0020\u0020sin2<i>x</i> = 2sin<i>x</i> cos<i>x</i> \n
        \u0020\u0020\u0020\u0020\u0020sin2<i>x</i> = 2tg<i>x</i> / (1 + tg<sup>2</sup><i>x</i>) = \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 = 2ctg<i>x</i> / (1 + ctg<sup>2</sup><i>x</i>) = \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 = 2 / (tg<i>x</i> + ctg<i>x</i>) \n
        \u0020\u0020\u0020\u0020\u0020cos2<i>x</i> = cos<sup>2</sup><i>x</i> - sin<sup>2</sup><i>x</i> = \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 = 2cos<sup>2</sup><i>x</i> - 1 = \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 = 1 - 2sin<sup>2</sup><i>x</i> \n
        \u0020\u0020\u0020\u0020\u0020cos2<i>x</i> = (1 - tg<sup>2</sup><i>x</i>) / (1 + tg<sup>2</sup><i>x</i>) = \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 = (ctg<sup>2</sup><i>x</i> - 1) / (ctg<sup>2</sup><i>x</i> + 1) = \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 = (ctg<i>x</i> - tg<i>x</i>) / (ctg<i>x</i> + tg<i>x</i>) \n
        \u0020\u0020\u0020\u0020\u0020tg2<i>x</i> = 2tg<i>x</i> / (1 - tg2<sup>2</sup><i>x</i>) = \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 = 2ctg<i>x</i> / (ctg<sup>2</sup><i>x</i> - 1) = \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 = 2 / (ctg<i>x</i> - tg<i>x</i>) \n
        \u0020\u0020\u0020\u0020\u0020ctg2<i>x</i> = (ctg<sup>2</sup><i>x</i> - 1) / 2ctg<i>x</i> = \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 = (ctg<i>x</i> - tg<i>x</i>) / 2 \n
        \u0020\u0020\u0020\u0020\u0020___________________________________ \n
        \u0020\u0020\u0020\u0020\u0020sin3<i>x</i> = 3sin<i>x</i> - 4sin<sup>3</sup><i>x</i> \n
        \u0020\u0020\u0020\u0020\u0020cos3<i>x</i> = 4cos<sup>3</sup><i>x</i> - 3cos<i>x</i> \n
        \u0020\u0020\u0020\u0020\u0020tg3<i>x</i> = (3tg<i>x</i> - tg<sup>3</sup><i>x</i>) / (1 - 3tg<sup>2</sup><i>x</i>) \n
        \u0020\u0020\u0020\u0020\u0020ctg3<i>x</i> = (ctg<sup>3</sup><i>x</i> - 3ctg<i>x</i>) / (3ctg<sup>2</sup><i>x</i> - 1) \n
        \u0020\u0020\u0020\u0020\u0020___________________________________ \n
        \u0020\u0020\u0020\u0020\u0020sin<sup>2</sup><i>x</i> = (1 - cos2<i>x</i>) / 2 \n
        \u0020\u0020\u0020\u0020\u0020cos<sup>2</sup><i>x</i> = (1 + cos2<i>x</i>) / 2 \n
        \u0020\u0020\u0020\u0020\u0020tg<sup>2</sup><i>x</i> = (1 - cos2<i>x</i>) / (1 + cos2<i>x</i>) \n
        \u0020\u0020\u0020\u0020\u0020ctg<sup>2</sup><i>x</i> = (1 + cos2<i>x</i>) / (1 - cos2<i>x</i>) \n
        \u0020\u0020\u0020\u0020\u0020sin<sup>2</sup>(<i>x</i>/2) = (1 - cos<i>x</i>) / 2 \n
        \u0020\u0020\u0020\u0020\u0020cos<sup>2</sup>(<i>x</i>/2) = (1 + cos<i>x</i>) / 2 \n
        \u0020\u0020\u0020\u0020\u0020tg<sup>2</sup>(<i>x</i>/2) = (1 - cos<i>x</i>) / (1 + cos<i>x</i>) \n
        \u0020\u0020\u0020\u0020\u0020ctg<sup>2</sup>(<i>x</i>/2) = (1 + cos<i>x</i>) / (1 - cos<i>x</i>) \n
        \u0020\u0020\u0020\u0020\u0020___________________________________ \n
        \u0020\u0020\u0020\u0020\u0020sin<sup>3</sup><i>x</i> = (3sin<i>x</i> - sin3<i>x</i>) / 4 \n
        \u0020\u0020\u0020\u0020\u0020cos<sup>3</sup><i>x</i> = (3cos<i>x</i> + cos3<i>x</i>) / 4 \n
        \u0020\u0020\u0020\u0020\u0020tg<sup>3</sup><i>x</i> = (3sin<i>x</i> - sin3<i>x</i>) / (3cos<i>x</i> + cos3<i>x</i>) \n
        \u0020\u0020\u0020\u0020\u0020ctg<sup>3</sup><i>x</i> = (3cos<i>x</i> + cos3<i>x</i>) / (3sin<i>x</i> - sin3<i>x</i>) \n

    </string>

    <string name="log_formulas">
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020<b>Properties of logarithms</b>  \n\n
        \u0020\u0020\u0020\u0020\u0020log<sub>a</sub>N = x &#8660;  a<sup>x</sup> = N \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 where a&#62;0, a&#8800;1 \n
        \u0020\u0020\u0020\u0020\u0020ln N = log<sub>e</sub>N , \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 where e &#8776; 2.718281828 - Eulerian number \n
        \u0020\u0020\u0020\u0020\u0020___________________________________ \n\n
        \u0020\u0020\u0020\u0020\u0020log<sub>a</sub>a = 1 \n
        \u0020\u0020\u0020\u0020\u0020log<sub>a</sub>1 = 0 \n
        \u0020\u0020\u0020\u0020\u0020log(a*b) = log a + log b \n
        \u0020\u0020\u0020\u0020\u0020log(a/b) = log a - log b \n
        \u0020\u0020\u0020\u0020\u0020log(a<sup>b</sup>) = b * log a \n

    </string>

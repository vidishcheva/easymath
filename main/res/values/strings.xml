    <string name="eq_formulas">
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020<b>Quadratic Equation</b>  \n\n
        \u0020\u0020\u0020\u0020\u0020ax<sub>2</sub>+bx+c = 0  \n
        \u0020\u0020\u0020\u0020\u0020D = b<sub>2</sub>-4ac   \n
        \u0020\u0020\u0020\u0020\u0020D&#62;0   \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020x<sup>1</sup> = (-b + &#8730;D)/2a  \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020x<sup>2</sup> = (-b - &#8730;D)/2a  \n
        \u0020\u0020\u0020\u0020\u0020D=0   \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020x<sup>1</sup>=x<sup>2</sup> = -b/2a  \n
        \u0020\u0020\u0020\u0020\u0020D&#60;0  \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020Equation has no solution  \n\n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020<b>Biquadratic Equation</b>  \n\n
        \u0020\u0020\u0020\u0020\u0020ax<sub>4</sub>+bx<sub>2</sub>+c = 0  \n
        \u0020\u0020\u0020\u0020\u0020solve equation with ay<sub>2</sub>+by+c=0 ,   \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020 where y = x<sub>2</sub> \n
        \u0020\u0020\u0020\u0020\u0020 x = &#8730;y \n
        \u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020Equation may have 0, 2 or 4 solutions  \n
    </string>
